<?xml version="1.0" encoding="utf-8"?>
<ipanelonline.mobile.survey.view.SquareRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                      android:orientation="vertical"
                                                      android:layout_width="wrap_content"
                                                      android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/main_gridView_item_photo"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerInParent="true"
        android:contentDescription="@null"
        android:padding="5dp"
        android:scaleType="fitXY" />

    <CheckBox
        android:id="@+id/main_gridView_item_cb"
        android:layout_width="56px"
        android:layout_height="56px"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_margin="2dp"
        android:background="@drawable/diary_dele"
        android:button="@null" />

</ipanelonline.mobile.survey.view.SquareRelativeLayout>